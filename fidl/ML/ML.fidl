package ford.rnd
 
interface ML {
  version {major 1 minor 0}

  method sayHello {
    in {
      <** @description : your name **>
      String name
    }
    out {
      String message
    }
  }

  method howMuch {
    out {
      UInt64 currency
    }
  }

  broadcast getOut {
    out {
      String phrase
    }
  }
} 
